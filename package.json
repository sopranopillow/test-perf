{
  "name": "test-perf",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "rimraf dist && rimraf lib && tsc && rollup --config rollup.config.js",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "preview": "vite preview",
    "test": "tensile --file dist/withSlotAlways.js --config tensile.config.js && tensile --file dist/withoutSlotAlways.js --config tensile.config.js",
    "all": "yarn build && yarn test",
    "generateHtml": "node computeComparison.mjs"
  },
  "dependencies": {
    "@fluentui/react-components": "^9.54.2",
    "react": "^17",
    "react-dom": "^17"
  },
  "devDependencies": {
    "@fluentui/react-jsx-runtime": "^9.0.39",
    "@rollup/plugin-commonjs": "^26.0.1",
    "@rollup/plugin-node-resolve": "^15.2.3",
    "@rollup/plugin-replace": "^5.0.7",
    "@tensile-perf/react": "^0.1.15",
    "@types/react": "^17",
    "@types/react-dom": "^17",
    "@typescript-eslint/eslint-plugin": "^7.2.0",
    "@typescript-eslint/parser": "^7.2.0",
    "@vitejs/plugin-react": "^4.2.1",
    "eslint": "^8.57.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.6",
    "path": "^0.12.7",
    "typescript": "^5.2.2",
    "vite": "^5.2.0",
    "rimraf": "*"
  }
}
